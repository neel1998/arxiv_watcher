{
  "manifest_version": 2,
  "name": "Arxiv Watcher",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": { "16": "logo192.png", "48": "logo512.png" },
  "permissions": ["storage", "tabs", "<all_urls>"]
}